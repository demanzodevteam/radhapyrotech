{
  "name": "radhapyrotech",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "migrate:dev": "dotenv -e .env.local -- npx prisma migrate dev",
    "db:push": "dotenv -e .env.local -- npx prisma db push",
    "db:pull": "dotenv -e .env.local -- npx prisma db pull",
    "migrate:reset": "dotenv -e .env.local -- npx prisma migrate reset",
    "db:seed": "dotenv -e .env.local -- npx prisma db seed",
    "prisma:generate": "dotenv -e .env.local -- npx prisma generate",
    "prisma:studio": "dotenv -e .env.local -- npx prisma studio",
    "production:build": "npx prisma generate && npx prisma migrate deploy && next build"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.4.2",
    "@prisma/client": "^5.14.0",
    "@react-pdf/renderer": "^3.4.4",
    "@tanstack/react-query": "^5.35.1",
    "@tanstack/react-query-devtools": "^5.35.1",
    "bcrypt": "^5.1.1",
    "cloudinary": "^2.2.0",
    "clsx-tailwind-merge": "^1.0.4",
    "dotenv-cli": "^7.4.2",
    "js-cookie": "^3.0.5",
    "next": "14.2.3",
    "next-auth": "^4.24.7",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.51.4",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^5.2.1",
    "react-spinners": "^0.13.8",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "eslint": "^8",
    "eslint-config-next": "14.2.3",
    "postcss": "^8",
    "prisma": "^5.14.0",
    "tailwindcss": "^3.4.1"
  }
}
